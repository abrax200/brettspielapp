{
    "root":{
        "height":"10px",
        "margin":"0px",
        "width":"100%",
        "box-sizing":"content-box",
        "padding":"0"
    },

    "handle":{
        "boxSizing":"border-box",
        "margin":"0",
        "boxShadow":"none",
        "backgroundColor":"var(--accent)",
        "height":"10px",
        "width":"10px",
        "border":"1px solid var(--border)"
        
    },
    "rail":{
            "backgroundColor":"transparent",
            "width":"100%",
            "height":"0px",
            "display":"none",
            "margin":"0px",
            "border":"1px solid var(--border)"
        },

    "track2":{
        "boxSizing":"border-box",
        "height":"10px",
        "margin":"0px",
        "zIndex":"1",
        "borderRadius":"0px",
        "borderTop":"1px solid var(--border)",
        "borderBottom":"1px solid var(--border)",
        "backgroundColor":"var(--accent)"}
}